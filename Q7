#include <iostream>
#include <vector>
using namespace std;
struct T{ int r,c,v; };
int main(){
    int rows, cols, non;
    if(!(cin>>rows>>cols>>non)) return 0;
    vector<T> t;
    t.push_back({rows,cols,non});
    for(int i=0;i<non;i++){ int r,c,v; cin>>r>>c>>v; t.push_back({r,c,v}); }
    vector<T> tr;
    tr.push_back({t[0].c,t[0].r,t[0].v});
    for(int col=0; col<t[0].c; ++col) for(size_t i=1;i<t.size();++i) if(t[i].c==col) tr.push_back({t[i].c,t[i].r,t[i].v});
    for(size_t i=0;i<tr.size();++i) cout<<tr[i].r<<" "<<tr[i].c<<" "<<tr[i].v<<"\n";
    return 0;
}
